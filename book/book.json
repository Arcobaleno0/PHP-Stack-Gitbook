{
    "title": "Sample Gitbook",
    "description": "Sample Gitbook to use as a base for additional book development",
    "gitbook": ">=2.3.2",
    "plugins":[
        "scrollablecode",
        "jsbin",
        "youtubex",
        "collapsible-menu",
        "codepen"
    ],
    "links": {
        "sharing": {
            "google": false,
            "facebook": false,
            "twitter": false,
            "all": false
        },
        "sidebar": {
            "Codeup": "http://www.codeup.com"
        }
    },
    "variables": {
        "curriculumName": ""
    },
    "search": {
        "maxIndexSize": 1000000000
    }
}
